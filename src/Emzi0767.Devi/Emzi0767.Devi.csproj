<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>A Discord selfbot</Description>
    <Copyright>Emzi0767 2017</Copyright>
    <AssemblyTitle>Dynamic Evaluation Implement (DEvI)</AssemblyTitle>
    <VersionPrefix>1.3.3</VersionPrefix>
    <TargetFrameworks>netcoreapp1.1;netcoreapp2.0</TargetFrameworks>
    <AssemblyName>Emzi0767.Devi</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>Emzi0767.Devi</PackageId>
    <RuntimeFrameworkVersion>1.1.1</RuntimeFrameworkVersion>
    <PackageTargetFallback>$(PackageTargetFallback);dnxcore50</PackageTargetFallback>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <Version>1.4.0</Version>
    <Authors>Emzi0767</Authors>
    <PackageProjectUrl>https://github.com/Emzi0767/Devi-Discord-Selfbot/</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/Emzi0767/Devi-Discord-Selfbot/blob/master/LICENSE.txt</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/Emzi0767/Devi-Discord-Selfbot/</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Discord, Discord.NET, Discord API, Discord bot, Discord selfbot, bot, selfbot, .NET, CSharp, Roslyn, Dynamic evaluation, Evaluation, Evaluator</PackageTags>
    <AssemblyVersion>1.4.0.0</AssemblyVersion>
    <FileVersion>1.4.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Emzi0767.AppDomainPolyfill\Emzi0767.AppDomainPolyfill.csproj" Condition=" '$(TargetFramework)'=='netcoreapp1.1' " />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="9.0.2-beta1" />
    <PackageReference Include="Microsoft.Extensions.DependencyModel" Version="1.1.0" />
    <PackageReference Include="System.Runtime.Loader" Version="4.3.0" />
    <PackageReference Include="Discord.Net" Version="1.0.0-rc-00610" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Scripting" Version="2.1.1-rc5-61428-03" />
    <PackageReference Include="Microsoft.CodeAnalysis.Scripting.Common" Version="2.1.1-rc5-61428-03" />
    <PackageReference Include="Microsoft.NETCore.App" Version="2.0.0-beta-001836-00" Condition=" '$(TargetFramework)'=='netcoreapp2.0' " />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\PublishProfiles\" />
  </ItemGroup>

</Project>
